#include <iostream>
#include <cmath>
using namespace std;

int main() {
    int n;
    cout << "Enter number: ";
    cin >> n;

    if (n == 0 || n == 1) {
        cout << "Digits: 1";
        return 0;
    }

    double sum = 0;
    for (int i = 2; i <= n; i++)
        sum += log10(i);

    cout << "Digits: " << (int)sum + 1;
    return 0;
}
