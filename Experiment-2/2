#include <stdio.h>

int main() {
    int n, i, j, maxAND = 0;
    int arr[100];

    printf("Enter number of elements: ");
    scanf("%d", &n);

    printf("Enter array elements:\n");
    for (i = 0; i < n; i++)
        scanf("%d", &arr[i]);

    for (i = 0; i < n; i++) {
        for (j = i + 1; j < n; j++) {
            int andValue = arr[i] & arr[j];
            if (andValue > maxAND)
                maxAND = andValue;
        }
    }

    printf("Maximum AND value = %d\n", maxAND);
    return 0;
}
